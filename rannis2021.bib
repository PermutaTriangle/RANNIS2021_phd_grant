@article{1161.05301,
	Abstract = {{Summary: Several authors have examined connections among 132-avoiding
    permutations, continued fractions, and Chebyshev polynomials of the second
    kind. In this paper we find analogues for some of these results for
    permutations $\pi$ avoiding 132 and $1\square 23$ (there is no occurrence
    $\pi_i<\pi_j<\pi_{j+1}$ such that $1\leq i\leq j-2$) and provide a
    combinatorial interpretation for such permutations in terms of lattice
    paths. Using tools developed to prove these analogues, we give enumerations
    and generating functions for permutations which avoid both 132 and $1\square
    23$, and certain additional patterns. We also give generating functions for
    permutations avoiding 132 and $1\square 23$ and containing certain
    additional patterns exactly once. In all cases we express these generating
    functions in terms of Chebyshev polynomials of the second kind.}},
	Author = {Hou, Qing-Hu and Mansour, Toufik},
	Classmath = {{05A05 (Permutations, words, matrices) 05A15 (Exact enumeration problems, generating functions) 33C45 (Orthogonal polynomials and functions of hypergeometric type) 11A55 (Continued fractions (number-theoretic results)) }},
	Date-Added = {2011-06-13 21:20:40 +0000},
	Date-Modified = {2011-06-13 21:20:40 +0000},
	Doi = {10.1016/j.dam.2005.11.004},
	Journal = {Discrete Appl. Math.},
	Keywords = {{restricted permutation; pattern-avoiding permutation; forbidden subsequence; continued fraction; Chebyshev polynomial}},
	Language = {English},
	Number = {8},
	Pages = {1183-1197},
	Reviewer = {{}},
	Title = {{Horse paths, restricted 132-avoiding permutations, continued fractions, and Chebyshev polynomials.}},
	Volume = {154},
	Year = {2006},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.dam.2005.11.004}}

